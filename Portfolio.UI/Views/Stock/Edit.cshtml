
@{
    ViewBag.Title = "Edit";
    //Layout = "~/Views/Shared/_Layout.cshtml";
}
<button class="navigate-back">Back to Stocks</button>
<br />
<h2>Edit</h2>
<div id="stockedit" class="content-container">
    <div data-bind="with: app.stockedit.stock">
        <label>Symbol:</label>
        <input data-bind="value: Symbol" />
        <br />
        <label>Price:</label>
        <input data-bind="value: LastPrice" />
        <br />
        <label>Company: </label>
        <input data-bind="value: CompanyName" />
    </div>
    <div>
        <button class="save-stock">Save</button>
    </div>
</div>

