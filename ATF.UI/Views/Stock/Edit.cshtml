@{
    ViewBag.Title = "Edit";
    //Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Edit</h2>
<button class="btn btn-default navigate-back">Back to Stocks</button>
<p></p>
<div id="stockedit" class="content-container">
    <div data-bind="with: app.stockedit.stock">
        @Html.Partial("_StockArea")
    </div>
    <div class="row col-sm-12">
        <div class="row col-sm-12">
            <div class="col-sm-4">
                <button class="btn btn-primary save-stock">Save</button>
            </div>
            <div class="col-sm-8">
                <button class="btn btn-danger" data-bind="click: app.stockedit.deleteStock">Delete</button>
            </div>
        </div>
    </div>
</div>

